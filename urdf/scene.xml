<mujoco model="Bot1_scene">
  <compiler angle="radian" meshdir="."/>
  <option timestep="0.002"/>
  <worldbody>
    <light pos="0 0 3" dir="0 0 -1" />
    <geom name="floor" type="plane" size="0 0 0.1" rgba="0.8 0.9 0.8 1"/>
    
    <!-- Include the URDF. 
         Note: MuJoCo's URDF loader might require the URDF to be valid and paths correct.
         We fixed paths in the previous step. -->
    <include file="robot.urdf"/>
  </worldbody>
</mujoco>
